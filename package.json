{
  "name": "babel-graphql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=latest",
    "start":
      "nodemon --nolazy --require babel-register --inspect src/index.js -e js,graphql",
    "debug":
      "nodemon --inspect -e js,graphql --exec npm run babel-node --debug -- ./index.js --watch src --debug",
    "build": "babel src -d dist --source-maps",
    "compile": "babel src --out-dir .compiled --source-maps --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "nodemon": "^1.17.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.1",
    "graphql-import": "^0.4.5",
    "graphql-serverless": "^0.7.5",
    "graphql-tools": "^2.23.0",
    "schemaglue": "^2.0.1"
  }
}
